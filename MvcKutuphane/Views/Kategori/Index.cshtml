@using MvcKutuphane.Models.Entity
@model List<TBLKATEGORI>    
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Layout.cshtml";
}


<h2>Kategoriler</h2>

<table class="table table-bordered">
    <tr>
        <th>Kategori ID</th>
        <th>Kategori Adı</th>
    </tr>

    @foreach (var ktg in Model)
    {
    <tr>
        <td>@ktg.ID</td>
        <td>@ktg.AD</td>
    </tr>

    }

</table>